{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "chimera",
      "camelcase": "Chimera",
      "title": "Chimera Lang",
      "scope": "source.chimera",
      "file-types": [
        "chi"
      ],
      "injection-regex": "^chimera$",
      "class-name": "TreeSitterChimera"
    }
  ],
  "metadata": {
    "version": "0.1.1",
    "license": "MIT",
    "description": "Chimera grammar for tree-sitter",
    "authors": [
      {
        "name": "Ilya Cherezov",
        "email": "ilya@cherezov.xyz"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-chimera"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
